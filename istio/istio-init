apiVersion: "batch/v1"
kind: "Job"
spec:
  backoffLimit: 6
  parallelism: 1
  completions: 1
  template:
    metadata:
      annotations:
        sidecar.istio.io/inject: "false"
      labels:
        app.kubernetes.io/component: "job.istio-init-crd-mixer-1.5.4"
    spec:
      restartPolicy: "OnFailure"
      terminationGracePeriodSeconds: 30
      nodeSelector: {}
      serviceAccountName: "istio-init-service-account"
      volumes:
      - name: "crd-mixer"
        configMap:
          name: "istio-crd-mixer"
      containers:
      - name: "istio-init-crd-mixer"
        image: "docker.io/istio/kubectl:1.5.4"
        command:
        - "kubectl"
        - "apply"
        - "-f"
        - "/etc/istio/crd-mixer/crd-mixer.yaml"
        args: []
        imagePullPolicy: "IfNotPresent"
        volumeMounts:
        - name: "crd-mixer"
          readOnly: true
          mountPath: "/etc/istio/crd-mixer"
        resources:
          limits:
            cpu: "100m"
            memory: "200Mi"
          requests:
            cpu: "10m"
            memory: "50Mi"
